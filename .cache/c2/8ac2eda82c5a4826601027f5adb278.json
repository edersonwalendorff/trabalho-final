\n            if (acao === 'reinicia') {\n                estado = 'parado';\n                clearInterval(timer);\n                contagem = 0;\n            }\n            break;\n\n        case 'rodando':\n\n            contagem = contagem + 0.1;\n            if (acao === 'pausa') {\n                estado = 'parado';\n                clearInterval(timer);\n            }\n            if (acao === 'reinicia') {\n                estado = 'parado';\n                clearInterval(timer);\n                contagem = 0;\n            }\n            break;\n    }\n\n    output.innerHTML = `${paraDecimalFixo(contagem)} s`;\n    \n}\n\nfunction paraDecimalFixo(num) {\n    return contagem.toFixed(1).replace('.', ',');\n}\n"},"lineCount":null}},"error":null,"hash":"294cad669b97722cf2d26833fafd691d","cacheData":{"env":{}}}